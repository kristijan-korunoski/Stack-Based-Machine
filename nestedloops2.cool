void nested_loops(void)
{
    int i = 1;

    while(3 - i) {
        int j = 1;

        while(4 - j) {
            int k = 1;

            while(5 - k) {
                print i;
                print j;
                print k; 

                k = k + 1;
            }
            j = j + 1;
        }
        i = i + 1;
    }
}

int main(void)
{
    nested_loops();
    return 0;
}
